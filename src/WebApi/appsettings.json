{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "FileExtensionsAllowed": [
    ".jpg",
    ".jpeg",
    ".png"
  ],
  "CdnSettings": {
    "Url": "https://api.cloudflare.com/client/v4/accounts/{account_id}/images/v1",
    "Token": "{token}"
  }
}